extends ./layout

mixin gauge(divId)
		.gauge
			div(id=divId)

block title  
	Title

block content
	script.
		window.app.gauge.init(!{JSON.stringify(sensors)});

	p #{sensors[0].gauges[0].id}

	.sensor
		h3 Sensor 1
		+gauge('gauge11')
		+gauge('gauge12')
	.sensor
		h3 Sensor 2
		+gauge('gauge21')
		+gauge('gauge22')
		