block title  
	Title

block content
	script.
		//window.app.device.init();
		let deviceEntries = !{JSON.stringify(deviceEntries)};
	div
		table
			thead
				tr
					td Datum
					td Sensor
					td Severity
					td Value
			tbody
				each deviceEntry in deviceEntries
					tr
						td #{deviceEntry.date}
						td #{deviceEntry.sensor.name}
						td #{deviceEntry.severity}
						td #{deviceEntry.value}